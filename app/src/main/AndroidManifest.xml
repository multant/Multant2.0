<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androstock.todotask">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title_home"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeToolBar">
        <activity
            android:name=".Home.HomeActivity"
            android:label="@string/title_activity_home">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.androstock.todotask.Task.TaskHome"></activity>
        <activity android:name="com.androstock.todotask.Task.AddTask"></activity>
        <activity android:name="com.androstock.todotask.chat.Chat_test"></activity>

    </application>

</manifest>